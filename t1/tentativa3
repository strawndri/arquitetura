    addi 1
    st r0, r1
    sub r0, r0

    add r1, r3
    addi 15
    addi 3
    st r0, r1
    sub r0, r0

    ; Preenchendo B ------

    addi 1
    add r1, r0

    sub r0, r0
    addi 1
    st r0, r1
    sub r0, r0

    add r1, r3
    addi 3
    st r0, r1
    sub r0, r0

