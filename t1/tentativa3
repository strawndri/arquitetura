    addi 9
    st r0, r1
    sub r0, r0

    add r1, r3
    addi 11
    st r0, r1
    sub r0, r0

    add r1, r3
    addi 13
    st r0, r1
    sub r0, r0

    add r1, r3
    addi 15
    st r0, r1
    sub r0, r0

    add r1, r3
    addi 15
    addi 2
    st r0, r1
    sub r0, r0

    add r1, r3
    addi 15
    addi 4
    st r0, r1
    sub r0, r0

    ; Preenchendo R ------

    addi 1
    add r1, r0

    sub r0, r0
    addi 0
    st r0, r1

    add r1, r3
    st r0, r1

    add r1, r3
    st r0, r1

    add r1, r3
    st r0, r1

    add r1, r3
    st r0, r1

    add r1, r3
    st r0, r1

