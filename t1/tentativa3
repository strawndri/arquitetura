    sub r0, r0  ; Zera R0
    sub r1, r1  ; Zera R1 (endereÃ§o do vetor)
    sub r2, r2  ; Zera R2 (valores)
    sub r3, r3  ; Zera R3 (iterador)

    addi 1
    add r3, r0

    sub r0, r0

    addi 15
    addi 15
    addi 15
    addi 15
    addi 15
    addi 15
    addi 15
    addi 15
    addi 15
    add r1, r0 

    ; Preenchendo A ------

    sub r0, r0
    st r0, r1

    add r1, r3
    addi 2
    st r0, r1
    sub r0, r0

    add r1, r3
    addi 3
    st r0, r1
    sub r0, r0

    add r1, r3
    addi 6
